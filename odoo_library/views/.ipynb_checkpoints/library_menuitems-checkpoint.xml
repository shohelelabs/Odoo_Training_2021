<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record model="ir.actions.act_window" id="book_list_action">
            <field name="name">Books</field>
            <field name="res_model">library.book</field>
            <field name="view_mode">tree,form,pivot</field>
            <field name="help" type="html">
                <p class="o_view_nocontent_smiling_face">
                    Create the first book info
                </p>
            </field>
        </record>
        <record model="ir.actions.act_window" id="reservation_list_action">
            <field name="name">Reservationss</field>
            <field name="res_model">library.reservation</field>
            <field name="view_mode">tree,form,calender</field>
            <field name="help" type="html">
                <p class="o_view_nocontent_smiling_face">
                    Create the first reservation info
                </p>
            </field>
        </record>
        <menuitem name="Odoo Library" id="menu_library" sequence="1"/>
        <menuitem id="menu_library_sub" name="Odoo Library Sub" parent="menu_library" sequence="2"/>
        <menuitem id="menu_library_books" name="Books" parent="menu_library_sub" sequence="3" action="book_list_action"/>
        
         <menuitem id="menu_library_reservations" name="Reservations" parent="menu_library_sub" sequence="4" action="reservation_list_action"/>
    </data>
</odoo>